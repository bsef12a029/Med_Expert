@{
    ViewBag.Title = "ClientRequests";
}
@model List<FINAL_EAD_PEOJECT.Models.ClientRequest>


<h2>
    <span class="label label-warning">List Of Request</span>
</h2>
<div align="right">
    <form method="post" action="/Doctor_Register/logout">
        <input type="submit" class="btn btn-warning" value="Log out" />
    </form>

</div>
<div>
    &nbsp;Doctor Name : @Session["doc_name"] &nbsp;&nbsp;    Doctor Email : @Session["doc_email"]<br /><br />
    &nbsp; Doctor Appionments : @Session["no-of app"] &nbsp;&nbsp; Doctor Phone#  : @Session["doc_phone"]
</div>
<br /><br />

<table cellpadding="3" border="1">
    <tr>
        <td><span class="label label-warning">Patient Name</span></td>
        <td>&nbsp;</td>
        
        <td><span class="label label-warning">Appoinment Dtae&Time</span></td>
        <td>&nbsp;</td>
    </tr>
   

    @foreach (var u in Model)
    {
        <tr>
            <td>@u.pat.User_name</td>
            <td>&nbsp;</td>
            <td>@u.app.Date_Time</td>
            <td>&nbsp;</td>
        </tr>
    }
</table>

